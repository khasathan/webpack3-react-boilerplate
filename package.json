{
  "name": "node-webpack-react-boilerplate",
  "version": "1.0.0",
  "description": "node-webpack-react-boilerplate",
  "main": "./src/index.js",
  "scripts": {
    "build:dev": "NODE_ENV=development npm run clean && webpack --config ./webpack.dev.js --progress --profile --colors",
    "start:dev": "npm run build:dev && NODE_ENV=development webpack-dev-server --config ./webpack.dev.js --open",
    "build:prod": "NODE_ENV=production npm run clean && webpack --config ./webpack.prod.js --progress --profile --colors",
    "start:prod": "npm run build:prod && NODE_ENV=production node ./server.js",
    "clean": "rm -rf ./dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Korkeat W. <korkeat.w@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.3",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.10",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "semantic-ui-css": "^2.3.1",
    "semantic-ui-react": "^0.80.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-lodash": "^3.3.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "circular-dependency-plugin": "^5.0.2",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-standard": "^3.1.0",
    "file-loader": "^1.1.11",
    "graceful-fs": "^4.1.11",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.10.2",
    "webpack-cli": "^2.1.4",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2"
  }
}